{
  "name": "frame",
  "version": "0.0.1",
  "description": "",
  "main": "app/index.js",
  "scripts": {
    "start": "node dist/server.js",
    "build": "npm run build:js && npm run build:css && npm run build:server",
    "dev": "concurrently 'npm run watch:js' 'npm run watch:css' 'npm run watch:serve'",
    "serve": "node dist/server.js",
    "watch:serve": "concurrently 'npm run watch:server' 'nodemon dist/server.js'",
    "build:server": "NODE_ENV=production webpack --config webpack.server.config.js --progress -p",
    "watch:server": "NODE_ENV=development webpack --config webpack.server.config.js -w",
    "build:js": "NODE_ENV=production webpack --progress -p",
    "watch:js": "NODE_ENV=production webpack -w",
    "build:css": "NODE_ENV=production postcss app/styles/main.css -o dist/public/main.css",
    "watch:css": "NODE_ENV=development postcss app/styles/main.css -w -o dist/public/main.css"
  },
  "author": "estrattonbailey",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "concurrently": "^3.4.0",
    "cssnano": "^3.10.0",
    "lodash-webpack-plugin": "^0.11.4",
    "nodemon": "^1.11.0",
    "postcss": "^6.0.2",
    "postcss-calc": "^6.0.0",
    "postcss-cli": "^4.1.0",
    "postcss-cssnext": "^2.11.0",
    "postcss-discard-comments": "^2.0.4",
    "postcss-import": "^10.0.0",
    "postcss-nested": "^2.0.2",
    "standard": "^10.0.2",
    "standard-loader": "^6.0.1",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {
    "body-parser": "^1.17.2",
    "compression": "^1.6.2",
    "contentful": "^4.4.4",
    "cors": "^2.8.3",
    "dotenv": "^4.0.0",
    "express": "^4.15.3",
    "lodash": "^4.17.4",
    "marked": "^0.3.6",
    "micro-grid": "^0.1.3",
    "prismjs": "^1.6.0",
    "promise-polyfill": "^6.0.2",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-hydrate": "^0.5.1",
    "react-hydrate-link": "^0.1.0",
    "react-loadable": "^4.0.3",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "source-map-support": "^0.4.15"
  }
}
